<aura:component controller= 'GetAccountRecords' implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
<div class="slds-text-body_regular"> Account Table With Records. </div>
<aura:handler name="init" value="{!this}" action="{!c.myActionCallApex}"/>
<aura:attribute name='columns' type= 'list'/>
<aura:attribute name='data' type= 'Object'/>
    <lightning:datatable 
                        keyField = 'id'
                        data ='{!v.data}' 
                        columns='{!v.columns}'
                         hideCheckboxColumn='true'                        
                         />
</aura:component>