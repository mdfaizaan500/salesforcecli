<aura:component controller="lightningDemo" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="Name" type="String" />
   <aura:handler name="init" value="{!this}" action="{!c.myAction}"/>
    <lightning:button label="ClientBase"  onclick="{! c.handleClick }"/>
    <lightning:button label="ServerApex"  onclick="{! c.handleClickapex }"/>
    <aura:attribute name="data" type="Object" />
    <aura:attribute name="columns" type="list" />
    
    <lightning:datatable 
                         keyField='Id'
                       data='{!v.data}' 
                        columns='{!v.columns}'
                         hideCheckboxColumn='true'
                         />
</aura:component>